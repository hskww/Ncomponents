{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --loose all --optional runtime --stage 0 --modules common src/ --out-dir app/ --retain-lines",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile"
  },
  "dependencies": {
    "thinkjs": "2.1.x",
    "babel-runtime": "6.x.x",
    "gulp": "^3.9.0",
    "webpack": "^1.12.9",
    "merge-stream": "^0.1.8",
    "run-sequence": "^1.1.1",
    "gulp-load-plugins": "^0.10.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-babel": "^5.1.0",
    "gulp-changed": "^1.2.1",
    "gulp-concat": "^2.6.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.1.1",
    "gulp-size": "^1.2.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "^6.4.0",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-stage-1": "6.x.x",
    "gulp-minify": "0.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1"
  }
}
